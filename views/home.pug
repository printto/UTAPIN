extends layout

block content
  // Slider
  section.section-slide
    .wrap-slick1.rs1-slick1
      .slick1
        .item-slick1(style='background-image: url(https://vignette.wikia.nocookie.net/utau/images/7/7b/พื้นใส.png/revision/latest?cb=20180127071041);')
          .container.h-full
            .flex-col-l-m.h-full.p-t-100.p-b-30
              .layer-slick1.animated.visible-false(data-appear='fadeInDown', data-delay='0')
                h3(style='color: white; -webkit-text-stroke: 1px orange;') This week current rank
                img(src='./images/rank/1.png')
              .layer-slick1.animated.visible-false(data-appear='fadeInUp', data-delay='800')
                h2.ltext-104.cl2.p-t-19.p-b-43.respon1(style='color: orange; -webkit-text-stroke: 3px white;')
                  | Printto Magicbeat
              .layer-slick1.animated.visible-false(data-appear='zoomIn', data-delay='1600')
                a.flex-c-m.stext-101.cl0.size-101.bg1.bor1.hov-btn1.p-lr-15.trans-04(href='product.html')
                  | View Profile
        .item-slick1(style='background-image: url(https://vignette.wikia.nocookie.net/vocaloid/images/5/5a/Teto.png/revision/latest/scale-to-width-down/2000?cb=20180910023449);')
          .container.h-full
            .flex-col-l-m.h-full.p-t-100.p-b-30
              .layer-slick1.animated.visible-false(data-appear='rollIn', data-delay='0')
                h3(style='color: white; -webkit-text-stroke: 1px #00BFFF;') This week current rank
                img(src='./images/rank/2.png')
              .layer-slick1.animated.visible-false(data-appear='lightSpeedIn', data-delay='800')
                h2.ltext-104.cl2.p-t-19.p-b-43.respon1(style='color: #00BFFF; -webkit-text-stroke: 3px white;')
                  | Teto Kasane
              .layer-slick1.animated.visible-false(data-appear='slideInUp', data-delay='1600')
                a.flex-c-m.stext-101.cl0.size-101.bg1.bor1.hov-btn1.p-lr-15.trans-04(href='product.html')
                  | View Profile
        .item-slick1(style='background-image: url(https://vignette.wikia.nocookie.net/p__/images/9/93/Defoko_Utane.jpg/revision/latest?cb=20130924201347&path-prefix=protagonist);')
          .container.h-full
            .flex-col-l-m.h-full.p-t-100.p-b-30
              .layer-slick1.animated.visible-false(data-appear='rotateInDownLeft', data-delay='0')
                h3(style='color: white; -webkit-text-stroke: 1px brown;') This week current rank
                img(src='./images/rank/3.png')
              .layer-slick1.animated.visible-false(data-appear='rotateInUpRight', data-delay='800')
                h2.ltext-104.cl2.p-t-19.p-b-43.respon1(style='color: brown; -webkit-text-stroke: 3px white;')
                  | Uta Utane
              .layer-slick1.animated.visible-false(data-appear='rotateIn', data-delay='1600')
                a.flex-c-m.stext-101.cl0.size-101.bg1.bor1.hov-btn1.p-lr-15.trans-04(href='product.html')
                  | View Profile
  // Banner
  .sec-banner.bg0
    .flex-w.flex-c-m
      .size-202.m-lr-auto.respon4
        // Block1
        .block1.wrap-pic-w.rank-pic(style='background-image: url(https://vignette.wikia.nocookie.net/utau/images/7/7b/พื้นใส.png/revision/latest?cb=20180127071041);')
